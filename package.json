{
  "name": "@louiss0/remark-astro-layout-url-injector",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"      
    }
  },
  "files": ["dist", "lib"],
  "version": "0.0.1",
  "scripts": {
    "start": "astro dev",
    "dev": "tsup lib/index.ts --watch",
    "build": "tsup lib/index.ts --format cjs,esm --dts --clean",
    "release": "npm run build && npm publish"
  },
  "devDependencies": {
    "astro": "^1.6.3"
  },
  "dependencies": {
    "tsup": "^6.4.0"
  }
}
